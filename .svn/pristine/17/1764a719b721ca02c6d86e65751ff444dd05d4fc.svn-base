/*轮播图*/
.slider {
    width: 100%;
    position: relative;
    overflow: hidden;
}

.slider ul:nth-child(1) {
    /*这个时候我们需要放下10张图片  我们宽度就要是父容器宽度的10倍*/
    width: 1000%;
    -webkit-transform: translateX(-10%);
    transform: translateX(-10%);
}

.slider ul:nth-child(1) li {
    width: 10%;
    float: left;
}

.slider ul:nth-child(1) li a {
    width: 100%;
    display: block;
}

.slider ul:nth-child(1) li a img {
    width: 100%;
    display: block;
}

.slider ul:nth-child(2) {
    position: absolute;
    /*绝对定位的盒子怎么居中 left 50% maegin-left 一半的宽度 而且是负的*/
    width: 118px;
    height: 10px;
    left: 55%;
    bottom: 5px;
    margin-left: -59px;
}

.slider ul:nth-child(2) li {
    float: left;
    width: 10px;
    height: 10px;
    border: 1px solid #fff;
    border-radius: 5px;
    margin-left: 5px;
}

.slider ul:nth-child(2) li.now {
    background: #fff;
}

.slider ul:nth-child(2) li:first-child {
    margin-left: 0;
}

