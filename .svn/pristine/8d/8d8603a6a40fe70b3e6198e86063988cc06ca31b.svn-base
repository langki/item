html, body {
    width: 100%;
    height: 100%;
}
* {
    margin: 0;
    padding: 0;
}

li {
    list-style: none;
    float: left;
}

a {
    text-decoration: none;
    color: #fff;
}

body {
    background: #1DA077;
}

.big {
    width: 100%;
    height: 100%;
    min-width: 300px;
    max-width: 640px;
    margin: 0 auto;
    position: relative;
    background: #1DA077;

}
.J-map-list, #allmap {
    width: 100%;
    height: 100%;
}

section {
    width: 100%;
    /*height: 800px;*/
    background: #1BA275;
}

.search {
    width: 94%;
    height: 39px;
    text-align: center;
    margin: 0 auto;
    border-radius: 6px;
    font-size: 18px;
    background: #fff;
    margin-top: 10px;
    margin-left: 3%;
    border: 1px solid #fff;
    color: #8E8E8E;

}

.search_one {
    position: absolute;
    left: 29%;
    top: 19px;
}

#triangle-down {
    width: 0;
    height: 0;
    border-left: 20px solid transparent;
    border-right: 20px solid transparent;
    border-top: 15px solid #fff;
    margin-top: -9px;
    margin-left: 39%;
}

.list {
    width: 94%;
    height: 40px;
    text-align: center;
    margin: 0 auto;
    background: #fff;
    margin-top: 10px;
    line-height: 47px;
    font-size: 18px;
}

.list_left {
    width: 50%;
    height: 40px;
    float: left;
    color: #1F9D77;
}

.list_left p {
    line-height: 47px;
}

.list_right {
    width: 50%;
    height: 40px;
    float: right;
    background: #8ECFBB;
    color: #FFFFFF;
}

.details {
    width: 94%;
    /*height: 430px;*/
    margin: 0 auto;
    margin-top: 20px;
    background: #69B0A4;
}

.details_top {
    height: 12px;

}

.details ul {
    margin-top: 5px;
    margin-left: 35%;
}

.details li {
    font-size: 18px;
    color: #fff;
}

.details li:nth-child(1) {
    margin-left: -32%;
}

.details li:nth-child(2) {
    margin-left: 43%;
}

.details_deep {
    height: 28px;

}

#triangle-left {
    width: 0;
    height: 0;
    border-left: 10px solid transparent;
    border-right: 10px solid transparent;
    border-top: 8px solid #fff;
    position: absolute;
    margin-top: -15px;
    margin-left: 68%;
}

#triangle-right {
    width: 0;
    height: 0;
    border-left: 10px solid transparent;
    border-right: 10px solid transparent;
    border-top: 8px solid #fff;
    margin-top: -15px;
    margin-left: 125%;
}

.details_one {
    width: 95%;
    height: 105px;
    margin: 0 auto;
    margin-top: 5px;
    background: #fff;
    border-radius: 8px 8px 0 0;

}

.details img {
    width: 38%;
    height: 92px;
    margin-top: 10px;
    margin-left: 2%;
}

.details_one h1 {
    margin-left: 44%;
    margin-top: -97px;
    font-size: 0.25rem;
    color: #357B85;
    margin-bottom: 27px;
}

.details_one p {
    margin-left: 44%;
    margin-top: -25px;
    font-size: 0.15rem;
    color: #8E8E8E;
    height: 18px;

}

.details_four {
    height: 24px;
    width: 46%;
    background: #FCE775;
    /*margin-left: 44%;*/
    /*margin-top: 15px;*/
    border-radius: 15px;
    float: left;

}

.details_four p {
    text-align: center;
    color: #FFAB17;

    margin: 3px 3px;
}

.details_four_one {
    width: 47%;
    height: 24px;
    background: #1BA375;
    /*margin-left: 6px;*/
    border-radius: 15px;
    float: right;
    margin-right: 2px;
}

.details_four_one p {
    color: #fff;
    text-align: center;
    margin: 4px -14px;
    font-size: 0.16rem;
}

.details_one .details_four li {
    color: #FFA200;
    margin-left: -3px;
}

.details_one .details_four ul {
    margin-left: 18%;
}

.details_six {
    width: 26%;
    height: 26px;
    background: #1BA375;
    margin-left: 73%;
    margin-top: 14px;
    border-radius: 28px;
    text-align: center;
}

.details_six p {
    color: #fff;
    margin-left: 10%;
    padding-top: 6px;
}

.details_deep select {
    display: inline-block;
    width: 28%;
    height: 28px;
    margin: 0 10%;
    text-align: center;
    float: left;
    color: #fff;
    font-size: 16px;
}

.details_deep option {
    text-align: center;
}

.details_four_two {
    width: 57%;
    float: right;
    margin-top: 8%;
}

.bttom {
    width: 100%;
    height: 30px;
    /*background: red;*/
    text-align: center;
    line-height: 30px;
}
